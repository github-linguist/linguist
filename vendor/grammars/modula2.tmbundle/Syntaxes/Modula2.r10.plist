<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>name</key>
    <string>Modula-2 R10</string>
	<key>scopeName</key>
	<string>source.modula2.r10</string>
	<key>fileTypes</key>
	<array>
		<string>.def</string>
		<string>.mod</string>
	<key>keyEquivalent</key>
	<string>^~M</string>
	</array>
	<key>firstLineMatch</key>
	<string>^[:blank:]*(*!m2r10(+[a-z]+)?*).*</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>name</key>
			<string>keyword.control</string>
			<key>match</key>
			<string>\b(CASE|ELSE|ELSIF|EXIT|FOR|IF|LOOP|REPEAT|RETURN|THEN|UNTIL|WHILE|YIELD)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>keyword.operator</string>
			<key>match</key>
			<string>\b(AND|DIV|IN|MOD|NOT|OR)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>keyword.other</string>
			<key>match</key>
			<string>\b(ALIAS|ARGLIST|ARRAY|BARE|BEGIN|BLUEPRINT|BY|CAST|CONST|COROUTINE|DEFINITION|DO|END|GENLIB|FROM|IMPLEMENTATION|IMPORT|MODULE|NEW|OF|OPAQUE|POINTER|PROCEDURE|QUALIFIED|RECORD|RELEASE|RETAIN|SET|TO|TYPE|VAR)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>constant.numeric.character</string>
			<key>match</key>
			<string>0u[0-9A-F]+('[0-9A-F]+)*</string>
		</dict>
		<dict>
			<key>name</key>
			<string>constant.numeric.base2</string>
			<key>match</key>
			<string>0b[01]+('[01]+)*</string>
		</dict>
		<dict>
			<key>name</key>
			<string>constant.numeric.base10</string>
			<key>match</key>
			<string>[0-9]+('[0-9]+)*(\.[0-9]+('[0-9]+)*(e[+-]?[0-9]+('[0-9]+)*)?)?</string>
		</dict>
		<dict>
			<key>name</key>
			<string>constant.numeric.base16</string>
			<key>match</key>
			<string>0x[0-9A-F]+('[0-9A-F]+)*</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.constant</string>
			<key>match</key>
			<string>\b(EMPTY|FALSE|NIL|TRUE)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.type</string>
			<key>match</key>
			<string>\b(BOOLEAN|CARDINAL|CHAR|UNICHAR|INTEGER|LONGCARD|LONGINT|LONGREAL|OCTET|REAL)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.function</string>
			<key>match</key>
			<string>\b(ABS|CHR|COUNT|LENGTH|INSERT|MAX|MIN|ODD|ORD|PTR|PRED|READ|REMOVE|SUCC|TMIN|TMAX|TSIZE|WRITE|WRITEF)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.class</string>
			<key>match</key>
			<string>\b(Storage|UNSAFE)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.type.nonpervasive</string>
			<key>match</key>
			<string>\b(ADDRESS|BYTE|WORD)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>support.function.nonpervasive</string>
			<key>match</key>
			<string>\b(ADDC|ADR|ALLOCATE|ASHR|BWAND|BWNOT|BWOR|DEALLOCATE|SHL|SHR|ROTL|ROTR|SUBC|SETBIT|TESTBIT)\b</string>
		</dict>
		<dict>
			<key>name</key>
			<string>string.quoted.single</string>
			<key>match</key>
			<string>'[^']*'</string>
		</dict>
		<dict>
			<key>name</key>
			<string>string.quoted.double</string>
			<key>match</key>
			<string>"[^"]*"</string>
		</dict>
		<dict>
			<key>name</key>
			<string>comment.block.dialecttag</string>
			<key>begin</key>
			<string>(*!</string>
			<key>end</key>
			<string>*)</string>
		</dict>
		<dict>
			<key>name</key>
			<string>comment.block.preprocessor</string>
			<key>begin</key>
			<string>(*?</string>
			<key>end</key>
			<string>*)</string>
		</dict>
		<dict>
			<key>name</key>
			<string>comment.block.pragma</string>
			<key>begin</key>
			<string><*</string>
			<key>end</key>
			<string>*></string>
		</dict>
		<dict>
			<key>name</key>
			<string>comment.block.untitled</string>
			<key>begin</key>
			<string>(*[^!\?]</string>
			<key>end</key>
			<string>*)</string>
		</dict>
	</array>
	<key>uuid</key>
	<string></string>
</dict>
</plist>
