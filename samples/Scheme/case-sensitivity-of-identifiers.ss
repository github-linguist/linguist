(define dog "Benjamin")
(define Dog "Samba")
(define DOG "Bernie")

(if (eq? dog DOG)
        (begin (display "There is one dog named ")
                (display DOG)
                (display ".")
                (newline))
        (begin (display "The three dogs are named ")
                (display dog) (display ", ")
                (display Dog) (display " and ")
                (display DOG)
                (display ".")
                (newline)))
