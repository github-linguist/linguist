(define input "((data \"quoted data\" 123 4.5)
 (data (!@# (4.5) \"(more\" \"data)\")))")
(define data (read (open-input-string input)))
(define output (let ((out (open-output-string)))
                  (write data out)
                  (get-output-string out)))
(write input) (newline)
(write data) (newline)
(write output) (newline)
