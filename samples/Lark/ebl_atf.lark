?start: line

?line: empty_line
     | note_line
     | text_line
     | dollar_line
     | at_line
     | control_line

empty_line: /\s+/?

!control_line.-2: ("=:" | "&" | "#") /.+/?

%import .ebl_atf_text_line (text_line, any_word, note_line)
%import .ebl_atf_dollar_line (dollar_line)
%import .ebl_atf_at_line (at_line)
