funcion promedio(a,b)
    retorno (a+b)/2
fin

funcion max(a,b)
    si (a > b)
        retorno a
    sino
        retorno b
    fin
fin

funcion min(a,b)
    si (a < b)
        retorno a
    sino
        retorno b
    fin
fin

imprimir(promedio(3, 5)) #4
imprimir(max(3, 5)) #5
imprimir(min(3, 5)) #3
