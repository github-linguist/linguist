// Addition
log(limath['+'](5, 3));  // Output: 8

// Subtraction
log(limath['-'](5, 3));  // Output: 2

// Division
console.log(limath['/'](6, 3));  // Output: 2

// Square root
log(limath['::'](9, 2));  // Output: 3

// Multiplication
log(limath['*'](5, 3));  // Output: 15

// Power
log(limath['~'](2, 3));  // Output: 8
