<#compress>
<#assign urls = [
    {
        "loc": "https://example.com/page1",
        "lastmod": "2025-08-28"
    },
    {
        "loc": "https://example.com/page2",
        "lastmod": "2025-08-29"
    }
] />

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <#list urls as url>
        <@urlTag url=url />
    </#list>
</urlset>
</#compress>

<#macro urlTag url>
    <url>
        <loc>${url.loc}</loc>
        <lastmod>${url.lastmod}</lastmod>
    </url>
</#macro>
