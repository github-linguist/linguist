using "fels.lang.sfm"
using "fels.lang.console"
using "fels.utils.random"


class Randomizer{
    func Randomizer() {} //constructor

    func randomNumber(min,max){
        return random.random(min,max)//generating and returning random number
    }
}

rnd = new Randomizer()//creating instance
fromNumber = console.readNum()//reading string
toNumber = console.readNum()
println(rnd.randomNumber(fromNumber,toNumber))