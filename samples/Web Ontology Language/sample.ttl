               rdfs:subClassOf :PizzaTopping ;
               owl:disjointWith :FishTopping ,
                                :FruitTopping ,
                                :HerbSpiceTopping ,
                                :MeatTopping ,
                                :NutTopping ,
                                :SauceTopping ,
                                :VegetableTopping ;
               rdfs:label "CoberturaDeQueijo"@pt .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#CheeseyPizza
:CheeseyPizza rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Pizza
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasTopping ;
                                                           owl:someValuesFrom :CheeseTopping
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Any pizza that has at least 1 cheese topping."@en ;
              rdfs:label "PizzaComQueijo"@pt .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#CheeseyVegetableTopping
:CheeseyVegetableTopping rdf:type owl:Class ;
                         rdfs:subClassOf :CheeseTopping ,
                                         :VegetableTopping ;
                         rdfs:comment "This class will be inconsistent. This is because we have given it 2 disjoint parents, which means it could never have any members (as nothing can simultaneously be a CheeseTopping and a VegetableTopping). NB Called ProbeInconsistentTopping in the ProtegeOWL Tutorial."@en ;
                         rdfs:label "CoberturaDeQueijoComVegetais"@pt .


