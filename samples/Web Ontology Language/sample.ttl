@prefix : <http://www.co-ode.org/ontologies/pizza/pizza.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.co-ode.org/ontologies/pizza/pizza.ttl> .

<http://www.co-ode.org/ontologies/pizza/pizza.ttl> rdf:type owl:Ontology ;
                                                    rdfs:comment "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
                                                    owl:versionInfo "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en ,
                                                                    "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en ,
                                                                    "v.1.6 modified for ttl"^^xsd:string .

#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#hasBase
:hasBase rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasIngredient ;
         owl:inverseOf :isBaseOf ;
         rdf:type owl:FunctionalProperty ,
                  owl:InverseFunctionalProperty ;
         rdfs:domain :Pizza ;
         rdfs:range :PizzaBase .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#hasCountryOfOrigin
:hasCountryOfOrigin rdf:type owl:ObjectProperty .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               owl:inverseOf :isIngredientOf ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :Food ;
               rdfs:range :Food ;
               rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#hasSpiciness
:hasSpiciness rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:range :Spiciness ;
              rdfs:comment "A property created to be used with the ValuePartition - Spiciness."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#hasTopping
:hasTopping rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            owl:inverseOf :isToppingOf ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain :Pizza ;
            rdfs:range :PizzaTopping ;
            rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#isBaseOf
:isBaseOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isIngredientOf ;
          rdf:type owl:FunctionalProperty ,
                   owl:InverseFunctionalProperty ;
          rdfs:domain :PizzaBase ;
          rdfs:range :Pizza .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#isIngredientOf
:isIngredientOf rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :Food ;
                rdfs:range :Food ;
                rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#isToppingOf
:isToppingOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isIngredientOf ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :PizzaTopping ;
             rdfs:range :Pizza ;
             rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"@en .


#################################################################
#    Classes
#################################################################

###  http://www.co-ode.org/ontologies/pizza/pizza.ttl#American
:American rdf:type owl:Class ;
          rdfs:subClassOf :NamedPizza ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTopping ;
                            owl:someValuesFrom :MozzarellaTopping
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTopping ;
                            owl:someValuesFrom :PeperoniSausageTopping
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTopping ;
                            owl:someValuesFrom :TomatoTopping
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTopping ;
                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                owl:unionOf ( :MozzarellaTopping
                                                              :PeperoniSausageTopping
                                                              :TomatoTopping
                                                            )
                                              ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasCountryOfOrigin ;
                            owl:hasValue :America
                          ] ;
          owl:disjointWith :AmericanHot ,
                           :Cajun ,
                           :Capricciosa ,
                           :Caprina ,
                           :Fiorentina ,
                           :FourSeasons ,
                           :FruttiDiMare ,
