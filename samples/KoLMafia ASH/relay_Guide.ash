    {
        turns_until_next_war_nc = (battlefield_turns + 7) % 7;
        if (turns_until_next_war_nc != 0)
        	turns_until_next_war_nc = 7 - turns_until_next_war_nc;
    }
    
    if (turns_until_next_war_nc == 0)
