    }
    //else
        //result.HTMLAppendDivOfClass(KOLImageGenerateImageHTML(entry.image_lookup_name, true, max_image_dimensions_large), "r_cl_l_left");
    
    
    //result.HTMLAppendTagPrefix("div", "class", "r_cl_l_right_container");
    
    
