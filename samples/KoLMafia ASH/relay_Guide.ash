			if (details != "")
				description.listAppend(details);
                
                
            if (url.length() == 0)
            {
                if (skills_to_urls contains s)
                    url = skills_to_urls[s];
                else
                    url = "skills.php";
            }
			
			subentries.listAppend(ChecklistSubentryMake(line, "", description));
