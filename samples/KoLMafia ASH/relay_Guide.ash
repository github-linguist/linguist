                    if (it.to_slot() != $slot[off-hand]) continue;
                    if (it.item_type() != "shield") continue;
                    if (!it.can_equip()) continue;
                    if (it.available_amount() == 0) continue;
                    if (it.get_power() > best_shield.get_power() || best_shield == $item[none])
                        best_shield = it;
                }
            */
